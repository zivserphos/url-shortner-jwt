{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "ignorePatterns": ["dist" ,"src/index.js"],
    "rules": {
        "no-throw-literal":"off",
        "linebreak-style": ["error" , "windows"],
        "no-console": "off",
        "no-restricted-syntax":"off",
        "no-restricted-imports":"off"
        
    }
}
